{
  "name": "@vue-styled-components/plugin",
  "type": "module",
  "version": "1.0.1",
  "author": {
    "name": "akinoccc",
    "email": "akinochen@foxmail.com"
  },
  "license": "Apache-2.0",
  "repository": {
    "url": "git+https://github.com/vue-styled-components/plugin.git"
  },
  "keywords": [
    "vue3",
    "styled-components",
    "styled-system",
    "css",
    "css-in-js",
    "typescript",
    "vite-plugin"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
